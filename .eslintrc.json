{
  "env": {
    "jest": true,
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "implicit-arrow-linebreak": "off",
    "linebreak-style": "off",
    "import/no-extraneous-dependencies": "off"
  }
}
